  RESPONSE #20
    SetGlobalTimer("BUSY","LOCALS",15)
    AttackOneRound(NearestEnemyOf(Myself))
  RESPONSE #20
    Wait(3)
    SetGlobalTimer("BUSY","LOCALS",12)
    Continue()
  RESPONSE #20
    Wait(3)
    SetGlobalTimer("BUSY","LOCALS",12)
    Continue()
END

