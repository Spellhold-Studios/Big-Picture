IF
  See([EVILCUTOFF])
  HasItem("ring28",LastSeenBy(Myself))  // ~Ring of Air Control~
  !HasItem("lich",LastSeenBy(Myself))  // <Invalid Strref -1>
  !Specifics(LastSeenBy(Myself),2001)
THEN
  RESPONSE #100
    ChangeSpecifics(LastSeenBy(Myself),2001)
    ActionOverride(LastSeenBy(Myself),ChangeAIScript("gblich1c",CLASS))
END

